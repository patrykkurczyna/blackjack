package pl.edu.agh.sr.server.impl;

import java.util.HashMap;

public class Player {
	private String name;
	private int score;
	private HashMap cards;
	
	public Player(String name) {
		this.name = name;
		this.score = 0;
		this.cards = new HashMap();
	}
	
	public Player() {
		this("COMPUTER");
	}
}
